# include <stdio.h>
int main(void)
{
    int i = 10;
    int k = 20;
    int m;
    
    m = (3>2) && (k=8);//非0就是真，真值就是1
	printf("m = %d, k = %d",m,k);//出来k是8
	
	m = （1>2) && (k=5)；
	//出来k是8，前面不成立，后面就不执行
	//当与的左边是假的时候右边不执行，因为已经确定结果。
	//或的左边是真的时候右边不执行
	
	int k = 3+2*4 && (5||6-4)//优先级查表或者加括号就行
	
	return 0;
}
